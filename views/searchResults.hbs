<h2>All Movies</h2>
<div class="movie-container">
    {{#each movies}}
        <div class="movie-item">
            <h3>{{this.Title}}</h3>
            <p><strong>Director:</strong> {{this.Director}}</p>
            <p><strong>Year:</strong> {{this.Year}}</p>
        </div>
    {{/each}}
</div>

<div class="no-movies">
    <p>No movies found.</p>
</div>
